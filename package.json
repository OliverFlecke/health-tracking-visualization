{
  "name": "health-tracking-visualization",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "build-main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.config.js",
    "build-renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.config.js",
    "build": "npm run build-main && npm run build-renderer",
    "start-renderer-dev": "webpack-dev-server --config webpack.renderer.dev.config.js",
    "start-main-dev": "webpack --config webpack.main.config.js && electron ./dist/main.js",
    "start-dev": "cross-env START_HOT=1 npm run start-renderer-dev",
    "prestart": "npm run build",
    "start": "concurrently \"npm:start-renderer-dev\" \"npm:start-main-dev\"",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --fix",
    "test": "jest '(\\/test\\/(?!e2e/)).*'",
    "pretest:e2e": "npm run build",
    "test:e2e": "jest '(\\/test\\/e2e/).*'",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test,mocks}/**/*.{js,ts,tsx,json,css,scss,md}": [
      "prettier --config ./.prettierrc --write",
      "git add"
    ],
    "{src,test,mocks}/**/*.{js,ts,tsx}": [
      "npm run lint",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.+\\.spec)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(s?css|sass)$": "<rootDir>/mocks/styleMock.js"
    }
  },
  "build": {
    "productName": "ProductName",
    "appId": "org.your.productname",
    "mac": {
      "category": "your.app.category.type"
    },
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "linux": {
      "target": "deb"
    },
    "win": {
      "target": "nsis"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:OliverFlecke/health-tracking-visualization.git"
  },
  "author": {
    "name": "Oliver Fleckenstein",
    "email": "Oliverfl@live.dk"
  },
  "license": "SEE LICENSE",
  "bugs": {
    "url": "https://github.com/OliverFlecke/health-tracking-visualization/issues"
  },
  "homepage": "https://github.com/OliverFlecke/health-tracking-visualization",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@hot-loader/react-dom": "^16.9.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^24.0.18",
    "@types/reach__router": "^1.2.4",
    "@types/react": "^16.9.2",
    "@types/react-datepicker": "^2.9.1",
    "@types/react-dom": "^16.9.0",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "^7.1.2",
    "@types/react-test-renderer": "^16.9.0",
    "@types/webdriverio": "^4.8.7",
    "@types/webpack-env": "^1.14.0",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.2",
    "cross-env": "^5.2.1",
    "css-loader": "^2.1.1",
    "electron": "^11.5.0",
    "electron-builder": "^20.44.4",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.7.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.9.0",
    "lint-staged": "^8.2.1",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.12.12",
    "react-test-renderer": "^16.9.0",
    "redux-devtools-extension": "^2.13.5",
    "sass-loader": "^7.3.1",
    "source-map-loader": "^0.2.4",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.19.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.6.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "date-fns": "^2.2.1",
    "fast-csv": "^3.4.0",
    "highcharts": "^7.2.0",
    "highcharts-react-official": "^2.2.2",
    "react": "^16.9.0",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-datepicker": "^2.9.6",
    "react-dom": "^16.9.0"
  }
}
